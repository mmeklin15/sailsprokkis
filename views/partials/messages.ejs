<% flash.get('success').forEach(function (message) { %>
    <div class="ui success message">
      <i class="close icon"></i>
      <%= message %>
    </div>
<% }) %>
<% flash.get('error').forEach(function (message) { %>
  <div class="ui error message">
    <i class="close icon"></i>
    <p><%= message %></p>
  </div>
<% }) %>